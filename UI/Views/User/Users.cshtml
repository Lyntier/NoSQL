@model IEnumerable<NoSQL.Models.User>
@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>Id </th>
        <th>First Name </th>
        <th>Last Name </th>
        <th>Email</th>
        <th>Tickets</th>
      
    </tr>
    @foreach (var u in Model)
    {
        <tr>
            <td>@u.FirstName</td>
            <td>@u.LastName</td>
            <td>@u.EmailAddress</td>

        </tr>

    }
</table>

